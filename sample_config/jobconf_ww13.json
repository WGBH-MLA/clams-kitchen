{
  "id": "FILL",
  "local_base": "/home/USER/",
  "results_dir": "stovetop/FILL",
  "def_path": "stovetop/FILL/batch.csv",
  "media_dir": "batchmedia",
  "cache_dir": ".cache",
  "start_after_item": 0,
  "end_after_item": null,
  "overwrite_mmif": false,
  "cleanup_media_per_item": true,
  "cleanup_beyond_item": 3,
  "filter_warnings": "ignore",
  "parallel": 0,
  "docker_gpus_all": true,
  "clams_apps": [
    { "image": "ghcr.io/clamsproject/app-whisper-wrapper:v13" }
  ],
  "clams_params": [
    {
      "pretty": "true",
      "modelSize": "turbo",
      "modelLang": "en",
      "conditionOnPreviousText": false,
      "noSpeechThreshold": 0.1,
      "runningTime": true,
      "hwFetch": true
    }
  ],
  "post_procs": [
    {
      "name": "transcript_converter",
      "artifacts": ["transcript_mmif", "tpme_mmif", "transcript_aajson", "tpme_aajson", "transcript_text", "tpme_text"]
    }
  ]
}
